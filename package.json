{
  "name": "@theregge/power2save",
  "version": "2.0.2",
  "description": "Save small amounts of data in array indexed with powers of 2",
  "type": "module",
  "main": "./dist/power2save.umd.cjs",
  "module": "./dist/power2save.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/power2save.js",
      "require": "./dist/power2save.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "prepublishOnly": "npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1"
  }
}
